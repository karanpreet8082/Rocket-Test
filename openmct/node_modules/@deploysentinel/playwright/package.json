{
  "name": "@deploysentinel/playwright",
  "license": "UNLICENSED",
  "version": "0.3.4",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "DeploySentinel"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production rollup -c rollup.config.js",
    "ci:unit": "jest --coverage",
    "dev:unit": "jest --watchAll",
    "ci:lint": "yarn lint && yarn tsc --noEmit",
    "dev": "rollup -c rollup.config.js -w",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@achrinza/node-ipc": "^10.1.7",
    "@babel/code-frame": "^7.18.6",
    "@deploysentinel/debugger-core": "^0.3.6",
    "adm-zip": "^0.5.10",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.1",
    "@types/achrinza__node-ipc": "npm:@types/node-ipc",
    "@types/adm-zip": "^0.5.0",
    "@types/babel__code-frame": "^7.0.3",
    "eslint-plugin-playwright": "^0.11.2"
  },
  "keywords": [
    "debug",
    "reporting",
    "dashboard",
    "automation",
    "browser",
    "playwright",
    "plugin",
    "test",
    "testing",
    "e2e",
    "end-to-end"
  ]
}
